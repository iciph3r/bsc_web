<div class="log new">
  <h1>Upload a Log</h1>

  <div class="form">
    <%= form_for @log do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.label :log %>
      <%= f.file_field :log %>

      <%= button_tag type: 'submit', class: 'btn', name: 'commit' do %>
        Upload your log!
      <% end %>
    <% end %>
  </div>
</div>
